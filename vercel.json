{
  "version": 2,
  "builds": [
    {
      "src": "./api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/health$",
      "dest": "./api/index.js"
    },
    {
      "src": "^/api/(.*)$",
      "dest": "./api/index.js"
    },
    {
      "src": "^/(.*)$",
      "dest": "./api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "public": true
}